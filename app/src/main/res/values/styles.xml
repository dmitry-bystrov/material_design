<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="BrownCyanTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/brown_cyan_primary</item>
        <item name="colorPrimaryDark">@color/brown_cyan_primary_dark</item>
        <item name="colorAccent">@color/brown_cyan_accent</item>
    </style>

    <style name="CyanDeepOrangeTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/cyan_deep_orange_primary</item>
        <item name="colorPrimaryDark">@color/cyan_deep_orange_primary_dark</item>
        <item name="colorAccent">@color/cyan_deep_orange_accent</item>
    </style>
</resources>
